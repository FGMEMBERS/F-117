<PropertyList>

  <path>brake_chute.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>DragChute</object-name>
    <property>orientation/pitch-deg</property>
    <factor>-1</factor>
    <center>
      <x-m>4.41763</x-m>
      <y-m>0</y-m>
      <z-m>1.57554</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>DragChute</object-name>
    <property>gear/gear/steering-norm</property>
    <factor>25</factor>
    <center>
      <x-m>4.41763</x-m>
      <y-m>0</y-m>
      <z-m>1.57554</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

</PropertyList>